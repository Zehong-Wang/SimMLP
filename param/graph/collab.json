{
    "dataset": "collab",

    "epochs": 30,
    "verbose": 10,
    "batch_size": 32,
    "deg4feat": true, 
    
    "pooling": "mean",
    
    "hidden_dim": 512,
    "pred_dim": 512,

    "enc_layers": 2,
    "enc_norm": "batch", 
    "enc_drop": 0.0,
    "res_enc": true,

    "proj_layers": 2,
    "proj_norm": "batch",
    "proj_drop": 0.0,
    "aggr_norm": "gcn",
    
    "pred_layers": 2, 
    "pred_norm": "batch",
    "pred_drop": 0.0,

    "learning_rate": 0.0005,
    "weight_decay": 0.0,

    "feature_mask": 0.75,
    "edge_mask": 0.75,

    "aug_rounds": 1,
    "recon_lambda": 10,

    "eta": 1.0,
    "aux_pos_ratio": 0
}